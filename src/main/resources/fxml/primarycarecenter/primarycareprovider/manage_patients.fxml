<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<fx:root prefWidth="1300.0" type="AnchorPane" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <children>
      <SplitPane fx:id="splitPane" dividerPositions="0.33479587943533" prefWidth="1300.0">
        <items>
          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="578.0" prefWidth="100.0">
               <children>
                    <TableView fx:id="tblPatients" layoutX="31.0" layoutY="57.0" prefHeight="535.0" prefWidth="371.0">
                     <columns>
                        <TableColumn fx:id="colPatientName" prefWidth="152.0" text="Patient Name" />
                        <TableColumn fx:id="colDob" prefWidth="121.0" text="Date Of Birth" />
                        <TableColumn fx:id="colGender" prefWidth="97.0" text="Gender" />
                     </columns>
                    </TableView>
                    <Button fx:id="btnAdd" layoutX="47.0" layoutY="18.0" mnemonicParsing="false" onAction="#add" prefHeight="26.0" prefWidth="64.0" style="-fx-background-color: #0087D5; -fx-text-fill: white;" text="Add" />
                  <Button fx:id="btnUpdate" disable="true" layoutX="132.0" layoutY="17.0" mnemonicParsing="false" onAction="#updatePatientButton" prefHeight="26.0" prefWidth="64.0" style="-fx-background-color: #0087D5; -fx-text-fill: white;" text="Update" />
                  <Button fx:id="btnDelete" disable="true" layoutX="217.0" layoutY="17.0" mnemonicParsing="false" onAction="#deletePatient" prefHeight="26.0" prefWidth="64.0" style="-fx-text-fill: white; -fx-background-color: #0087D5;" text="Delete" />
                  <Button fx:id="btnStartVisit" layoutX="299.0" layoutY="17.0" mnemonicParsing="false" onAction="#startVisit" prefHeight="26.0" prefWidth="78.0" style="-fx-background-color: #0087D5; -fx-text-fill: white;" text="Start Visit" />
               </children>
            </AnchorPane>
          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="160.0" prefWidth="100.0" />
        </items>
      </SplitPane>
    </children>
</fx:root>
